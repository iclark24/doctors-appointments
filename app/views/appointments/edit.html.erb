<h1>Edit Appointment</h1>
<%= form_with(model: [@physician, @appointment], local: true, class: "ui form" ) do |f| %>
  <%= f.label :physician %>
  <%= f.select :physician_id, @physicians.collect { |p| [p.full_name, p.id] } %>
  <%= f.label :date%>
  <%= f.date_field :date%>
  <%= f.label :time%>
  <%= f.time_field :time%>
  <br />
  <%= f.submit nil, class: "ui green button inverted" %>
<% end %>